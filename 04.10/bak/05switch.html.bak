 <body>
 	<script language="JavaScript">
		day="수"
		switch(day){
			case "월" : {
				document.write("윤재환");
			}
			case "화" : {
				document.write("서지원");
			}
			case "수" : {
				document.write("민재홍");
			}
			case "목" : {
				document.write("김정원");
			}
			default : {
				document.write("김광일, 김유 출석");
			}
		}
	</script>
  
