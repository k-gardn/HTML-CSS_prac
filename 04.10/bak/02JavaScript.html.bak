 <body>
 		<script language="JavaScript">
 				//""있으면 문자로 인식
 				//""없으면 수식(등식, 부등식 등)으로 인식
 					a = "10"
 					b = "20"
 					c= a + b
 					document.write(c);
 					document.write("<br>");

					a = 10
 					b = 20
 					c= a + b				
					document.write(c,"이지롱~");
 					document.write("<br>");
					document.write(c + "일껄?");
					

					//대입연산자
					c = c + "이냐?" //변수를 이용해서 대입
					document.write(c,"<br><br>");

					//증가연산자
					a=1
					document.write(a++,"<br>");
					document.write(a++,"<br>");
					document.write(a++,"<br>");
					document.write(a++,"<br>");
					document.write(a++,"<br>");
					document.write(a++,"<br><br><br><br>");

					//+a: 증가를 하고 출력
					document.write(++a,"<br>");
					document.write(++a,"<br>");
					document.write(++a,"<br>");
					document.write(++a,"<br>");
					document.write(++a,"<br>");
					document.write(++a,"<br><br><br><br>");

 				</script>
  
 </body>
